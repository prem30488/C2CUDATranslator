    #include<stdio.h>
    #include<conio.h>
    #include<string.h>

    
    int main(int argc , char *argv [] )
    {
		char ch;
		kernel char x[20];
		kernel char y[20];
		kernel int m;
		kernel int n;
		int i;
		int j;
		kernel int c[20];
		kernel char b[20];

		for(i = 0;i<=m;i++)
		{ 
			c[i][0]=0;
		}
		for(i = 0;i<=n;i++)
		{
			c[0][i]=0;
		}
		#pragma kernel_start
		for(i = 1;i<=m;i++)
		{

			for (j = 1; j < 20; j++)
				{   
           
					if(x[i-1]==y[j-1])
					{
						row[j] = prevrow[j-1]+1;
						rowb[j] = c; //c stands for left upright cross
					}
					else if(x[j-1]>=x[j-2])
					{
						row[j] = prevrow[j];
						rowb[j] = u; //u stands for upright or above
					}
					else
					{
						row[j] = row[j-1];
						rowb[j] = l; //l stands for left
					}
				}
        
			
		}
		#pragma kernel_end
		
		printf("\nlcs are:");;
		//print_lcs(b,x,m,n);
		printf("\n Matrix is :: \n");;

		for(i = 1;i<=m;i++)
		{
			for(j = 1;j<=n;j++)
			{
				printf(" %d ",c[i][j]);;

			}
			printf("\n");;
		}
		
		return 0;
    } 
