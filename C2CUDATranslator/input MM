    #include<stdio.h>
    #include<conio.h>
    #include<string.h>
      
    #define ROWSIZE 3    
    #define COLSIZE 3    
    int main(int argc , char *argv [] )
    {
		int i;
		int j;
		int k;
		int sum;
		
		kernel int a[ROWSIZE][COLSIZE];
		kernel int b[ROWSIZE][COLSIZE];
		kernel int c[ROWSIZE][COLSIZE];
		
		for(i = 0;i<ROWSIZE;i++)
		{
			for(j = 0;j<COLSIZE;j++)
			{
				a[i][j] = 1;
				b[i][j] = j;
				c[i][j]=0;
			}
		}
		printf("\n A = ");;
		for(i = 0;i<ROWSIZE;i++)
		{
			printf("\n");;
			for(j = 0;j<COLSIZE;j++)
			{
				printf(" %d ",a[i][j]);;
			}
		}

		printf("\n B = ");;
		for(i = 0;i<ROWSIZE;i++)
		{
			printf("\n");;
			for(j = 0;j<COLSIZE;j++)
			{
				printf(" %d ",b[i][j]);;
			}
		}

		#pragma kernel_start
		
		for(i = 0;i<ROWSIZE;i++)
		{
			for(j = 0;j<COLSIZE;j++)
			{
			sum=0;
				for(k = 0;k<ROWSIZE;k++)
				{
					sum+=a[i][k]*b[k][j];
				}
			c[i][j]=sum;
			}
		}
		
		#pragma kernel_end
		
		printf("\n C = ");;
		for(i = 0;i<ROWSIZE;i++)
		{
			printf("\n");;
			for(j = 0;j<COLSIZE;j++)
			{
				printf(" %d ",c[i][j]);;
			}
		}

		return 0;
    } 
